{
  "name": "api",
  "version": "1.18.0",
  "description": "",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n watch,dev,inngest 'npm run build:watch' 'npm run dev:api' 'npm run dev:inngest'",
    "dev:api": "PORT=3001 nodemon dist/server.js | pino-pretty",
    "dev:inngest": "npx inngest-cli@latest dev -u http://localhost:3000/inngest",
    "build": "tsc --build",
    "build:watch": "tsc --build --watch --preserveWatchOutput",
    "start": "node ./dist/server.js",
    "lint": "eslint --cache --fix ."
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "eslint --cache --fix ."
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/multipart": "^7.7.3",

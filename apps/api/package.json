    "@rate-my-openapi/core": "*",
    "@sendgrid/mail": "^7.7.0",
    "@slack/web-api": "^6.9.0",
    "@stoplight/spectral-core": "^1.18.3",
    "@stoplight/spectral-parsers": "^1.0.3",
    "@stoplight/spectral-ruleset-bundler": "^1.5.2",
    "dotenv": "^16.3.1",
    "es-main": "^1.2.0",
    "fastify": "^4.21.0",
    "inngest": "^3.6.0",
    "js-yaml": "^4.1.0",
    "pino": "^8.15.1",

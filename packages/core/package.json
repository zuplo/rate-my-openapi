{
  "name": "@rate-my-openapi/core",
  "version": "1.18.0",
  "description": "Core utilities for producing an OpenAPI rating",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint --cache --fix ."
  },
  "author": "Zuplo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuplo/rate-my-openapi.git"
  },
  "bugs": {
    "url": "https://github.com/zuplo/rate-my-openapi/issues"
  },
  "homepage": "https://github.com/zuplo/rate-my-openapi#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.18.10",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "openapi-types": "^12.1.3"
  },
  "files": [
    "dist/**/**/*",
